@viewModel DotvvmAcademy.Course.Counter.CounterViewModel

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
</head>
<body>
    <p>{{value: Result}}</p>
    <dot:TextBox Text="{value: Difference}" />
    <dot:Button Click="{command: Add()}"
                Text="Add"/>
    <dot:Button Click="{command: Subtract()}"
                Text="Subtract"/>
</body>
</html>
